{
  "name": "nb-trans-demo",
  "version": "13.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:lib": "ng build --watch nb-trans",
    "build:lib": "ng build nb-trans && npm run copy:readmeEn && npm run copy:changelog && npm run copy:license",
    "build:analyze": "npm run build -- --source-map && npm run view:analyze",
    "view:analyze": "node_modules/.bin/source-map-explorer dist/nb-trans-demo/*",
    "test:lib": "ng test nb-trans",
    "deploy": "ng build --outputPath=docs --base-href=/nb-trans/ && cp docs/index.html docs/404.html",
    "dev:ssr": "ng run nb-trans-demo:serve-ssr",
    "serve:ssr": "node dist/nb-trans-demo/server/main.js",
    "build:ssr": "ng build && ng run nb-trans-demo:server",
    "prerender": "ng run nb-trans-demo:prerender",
    "publish:lib": "npm run build:lib && cd ./dist/nb-trans && npm publish --access=public",
    "copy:readmeEn": "cp projects/nb-trans/README.EN.md dist/nb-trans/",
    "copy:changelog": "cp CHANGELOG.* dist/nb-trans/",
    "copy:license": "cp LICENSE dist/nb-trans/",
    "pack:lib": "cd ./dist/nb-trans && npm pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.5",
    "@angular/common": "~13.3.5",
    "@angular/compiler": "~13.3.5",
    "@angular/core": "~13.3.5",
    "@angular/forms": "~13.3.5",
    "@angular/platform-browser": "~13.3.5",
    "@angular/platform-browser-dynamic": "~13.3.5",
    "@angular/platform-server": "~13.3.5",
    "@angular/router": "~13.3.5",
    "@bigbear713/nb-common": "^13.0.0",
    "@nguniversal/express-engine": "^13.1.0",
    "express": "^4.15.2",
    "lodash-es": "^4.17.21",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.4",
    "@angular/cli": "~13.3.4",
    "@angular/compiler-cli": "~13.3.5",
    "@nguniversal/builders": "^13.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.8.0",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.3.1",
    "source-map-explorer": "^2.5.2",
    "typescript": "~4.6.4"
  }
}
